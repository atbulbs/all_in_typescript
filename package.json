{
  "name": "all_in_typescript",
  "version": "1.0.0",
  "description": "project build all in typescript",
  "main": "main.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.ts",
    "build": "npm run clean && npm run build:prod",
    "build-dev": "npm run clean && npm run build:dev",
    "build:prod": "cross-env NODE_ENV=production webpack --config build/webpack.config.ts",
    "build:dev": "cross-env NODE_ENV=development webpack --config build/webpack.config.ts",
    "clean": "rimraf output"
  },
  "config": {
    "TS_NODE_PROJECT": "\"build/webpack.tsconfig.json\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atbulbs/all_in_typescript.git"
  },
  "keywords": [
    "TypeScript",
    "TS",
    "webpack"
  ],
  "author": "JasonLiao <atbulbs@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atbulbs/all_in_typescript/issues"
  },
  "homepage": "https://github.com/atbulbs/all_in_typescript#readme",
  "devDependencies": {
    "@types/node": "^12.7.3",
    "@types/webpack": "^4.39.1",
    "@types/webpack-dev-server": "^3.1.7",
    "cross-env": "^5.2.1",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
